{
	"$schema":"http://json-schema.org/draft-07/schema#",
	"title":"What2Play Add Games Schema",
	"description":"Request payload for /games/add",
	"type":"object",
	"properties":{
        "game_name": {
            "type": "string",
            "description": "game name"
		},
		"icon_url": {
			"type": "string",
			"description": "url to an icon to display"
		},
		"description": {
			"type": "string",
			"description": "description of this game"
		}
	},
	"required": [
		"game_name"
	]
}